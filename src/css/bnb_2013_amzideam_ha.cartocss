  }

  [dn=2]{
  polygon-fill: #FF9966;
  line-color: #663333; //ff6633;
  //polygon-comp-op: contrast;
  }

  [dn=3]{
  polygon-fill: #330000; //663333;
  line-color: #330033;
  }

  }
