//polygon-comp-op: overlay;
 //comp-op: dst;//plus; grain-extract;//hue;//invert-rgb; //exclusion;//difference; //contrast;//hard-light; //

  polygon-opacity: 0.9;
  //polygon-gamma-method: multiply;
  line-opacity: 0.25;
  line-width: 0;
  [zoom>=5] {line-width: 0.1;}
